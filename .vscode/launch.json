{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/GemKeeper/bin/Debug/net9.0/GemKeeper.dll",
      "args": ["-f", "takeout.zip", "--output", "output/"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "preLaunchTask": "build"
    }
  ]
}